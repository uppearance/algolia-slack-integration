{
  "stagingenv": {
    "app_function": "trend-register.app",
    "aws_environment_variables": {
      "ALGOLIA_APPLICATION_ID": "",
      "ALGOLIA_PRODUCT_INDEX_NAME": "",
      "ALGOLIA_SEARCH_API_KEY": "",
      "ALGOLIA_VIDEO_INDEX_NAME": "",
      "SLACK_TEAM_ID": "",
      "SLACK_VERIFICATION_TOKEN": ""
    },
    "aws_region": "ap-northeast-2",
    "exclude": [
      "__pycache__",
      ".git/*",
      ".gitignore",
      ".python-version",
      "LICENSE",
      "README.md",
      "requirements.txt",
      "zappa_settings.json"
    ],
    "keep_warm": true,
    "keep_warm_expression": "rate(15 minutes)",
    "memory_size": 128,
    "profile_name": "zappauser-staging",
    "project_name": "trending-keyword-register",
    "runtime": "python3.8",
    "s3_bucket": "zappa-trend-register",
    "timeout_seconds": 30
  }
}
